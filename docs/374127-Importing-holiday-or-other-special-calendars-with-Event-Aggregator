        <h1 class="inner-header" itemprop="name">Importing holiday or other special calendars with Event Aggregator</h1>
        <meta itemprop="author" content="The Events Calendar">
		<meta itemprop="headline" content="Importing holiday or other special calendars with Event Aggregator">   
		<meta itemscope itemprop="mainEntityOfPage" itemType="https://schema.org/WebPage" itemid="//support.theeventscalendar.com/374127-Importing-holiday-or-other-special-calendars-with-Event-Aggregator"/>                  
         <span itemprop="publisher" itemscope itemtype="https://schema.org/Organization">    	
    		<meta itemprop="name" content="The Events Calendar">
          <span itemscope itemprop="logo" itemtype="https://schema.org/ImageObject">
          	<meta itemprop="url" content="/scripts/file.php%3Fview=Y&file=7ac92ed24735aaa35e80b7775041b38b">
          	<meta itemprop="width" content="300px">
          	<meta itemprop="height" content="200px">
          </span>		
  		 </span>
        
                <!-- breadcrumbs -->
